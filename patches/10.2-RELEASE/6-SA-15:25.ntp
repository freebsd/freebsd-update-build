-	static u_int32 my_tick_to_tsf(u_int32 ticks);
-	static u_int32 my_tsf_to_tick(u_int32 tsf);
-
-	// that's it...
-	struct lfpfracdata {
-		long	nsec;
-		u_int32 frac;
-	};
