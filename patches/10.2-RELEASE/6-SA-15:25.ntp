-int stringreader(void* farg)
-{
-	const char ** cpp = (const char**)farg;
-	if (**cpp)
-		return *(*cpp)++;
-	else
-		return EOF;
-}
-
-static int/*BOOL*/
-setup_load_table(
-	const char * cp,
-	int          blim=FALSE)
-{
-	int            rc;
-	leap_table_t * pt = leapsec_get_table(0);
-	rc = (pt != NULL) && leapsec_load(pt, stringreader, &cp, blim);
-	rc = rc && leapsec_set_table(pt);
-	return rc;
-}
-
-static int/*BOOL*/
-setup_clear_table()
-{
-	int            rc;
-	leap_table_t * pt = leapsec_get_table(0);
-	if (pt)
-		leapsec_clear(pt);
-	rc = leapsec_set_table(pt);
-	return rc;
-}
